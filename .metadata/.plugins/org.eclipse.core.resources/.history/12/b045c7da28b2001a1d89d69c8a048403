package com.random;

public class RandomTest {

	public static void main(String[] args) {
		//임의 수 출력하는 랜덤 사용하기
		//Math클래스에 random() 메소드가 있음
		System.out.println((int)(Math.random()*10)); //한 번 실행할 때마다 실수 임의의 값(0.0~1.0사이) 나옴
		System.out.println((int)(Math.random()*10)); //그냥 int로 바꾸면 0이 나옴..
		System.out.println((int)(Math.random()*10));
		System.out.println((int)(Math.random()*10));
		
		
	}

}
