package com.oop.main;

import com.oop.model.vo.Car;

public class Main {
	public static void main(String[] args) {
		
		Car car1 = new Car(); 
		
		System.out.println(car1.getName() + car1.getGas() + 
				car1.getDoor()+ car1.getColor());
		
		System.out.print("모델명 : ");
		car1.setName("테슬라");
		car1.setGas(5000);
		car1.setDoor(5);
		car1.setColor("black");
		
		System.out.println(car1.getName() + car1.getGas() + 
				car1.getDoor()+ car1.getColor());
		
	}

}
