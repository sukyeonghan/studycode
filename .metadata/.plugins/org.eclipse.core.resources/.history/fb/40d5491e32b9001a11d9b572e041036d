package com.first.test;

public class Test {
	
	private String name="안녕"; //명시적 초기값
	
	{ 
		this.name="Rclass";
		System.out.println("초기화 블록 실행");
	} //초기화블록
	
	public void className() {
		this.name="몰라";
		System.out.println("생성자 초기화");
		
		
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}	

	
	
}
