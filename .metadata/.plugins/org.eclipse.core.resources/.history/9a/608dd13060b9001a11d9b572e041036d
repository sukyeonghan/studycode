package com.kh.model.vo;

public class Student extends Person {
	
	private int grade;
	private int rclass;
	private int number;
	
	
	public Student() {
		// TODO Auto-generated constructor stub
	}


	public Student(int grade, int number) {
		super();
		this.grade = grade;
		this.number = number;
	}
	
	


	public Student(String name, int age, char gender, int grade, int rclass, int number) {
		super(name, age, gender);
		this.grade = grade;
		this.rclass = rclass;
		this.number = number;
	}


	public int getGrade() {
		return grade;
	}


	public void setGrade(int grade) {
		this.grade = grade;
	}


	public int getNumber() {
		return number;
	}


	public void setNumber(int number) {
		this.number = number;
	}
	

}
