package com.random;

public class RandomTest {

	public static void main(String[] args) {
		//임의 수 출력하는 랜덤 사용하기
		//Math클래스에 random() 메소드가 있음
		System.out.println(Math.random());
		System.out.println(Math.random());
		System.out.println(Math.random());
		System.out.println(Math.random());
		
		
	}

}
