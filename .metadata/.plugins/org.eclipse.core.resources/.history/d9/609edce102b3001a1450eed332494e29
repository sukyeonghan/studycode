package test.array.premitive;

public class ArraySample {
	
	public void test1() {
		//10개의 int배열 선언하고 할당
		//각 방에 1~100 사이의 임의의 난수를 기록
		//10개의 정수들의 합계를 구하여 출력하기
		
		int [] su = new int [10];
		int sum = 0;
		
		for(int i=0; i<10; i++) {
			su[i] = ((int)(Math.random()*100)+1);
			sum+=su[i];
		}
		System.out.println(sum);
	}

}
